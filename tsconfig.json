{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "airkit-ui": ["components/index.ts"],
      "airkit-ui/es/*": ["components/*"],
      "airkit-ui/*": ["src/*", "*"]
    }
  },
  "exclude": ["node_modules"],
  "include": [".dumi/**/*", ".dumirc.ts", "components/**/*"]
}
